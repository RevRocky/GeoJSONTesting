    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Simplify Zones",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/scripts/smoosh_together/main.js"
        },     
        {
            "type": "node",
            "request": "launch",
            "name": "Augment Neighbourhood Info",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/scripts/augment_neighbourhood_data/augment_neighbourhood_data.js"
        },  
        {
            "type": "node",
            "request": "launch",
            "name": "Zone the Hoods",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/scripts/zone_the_neighbourhoods/zone_the_neighbourhoods_1.js"
        },    
        {
            "type": "node",
            "request": "launch",
            "name": "Zone the Hoods 2",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/scripts/zone_the_neighbourhoods/zone_the_neighbourhoods_2.js"
        },          
        {
            "type": "node",
            "request": "launch",
            "name": "Filter Geography (Toronto)",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "-s", "${workspaceFolder}/data/SimpleZonesV1.geojson",
                "-f", "${workspaceFolder}/data/TorontoBoundary.geojson",
                "-o", "${workspaceFolder}/data/TorontoCensusTracts.geojson",

            ],
            "program": "${workspaceFolder}/scripts/filter_geography/filter_geography.js"
        }
    
    ]
}